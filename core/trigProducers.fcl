}

TrigCalProducers : {
   CaloHitMakerFast:
   {
      module_type   : "CaloHitsFromFragments"
      diagLevel     : 0
      caloTag       : "" #"daq:calo"
      digiSampling  : 5
      deltaTPulses  : 5.
      nPEperMeV     : 30
      noiseLevelMeV : 0.55
      nSigmaNoise   : 4
      # hitEDepMax    : 80.
